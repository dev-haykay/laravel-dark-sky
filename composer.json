{
    "name": "illuminated/dark-sky",
    "description": "Dark Sky weather forecast for Laravel.",
    "keywords": ["laravel", "darksky", "dark", "sky", "weather", "forecast", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.7.*",
        "nesbot/carbon": "^1.26.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\DarkSky\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/DarkSky/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminated\\DarkSky\\ServiceProvider"
            ],
            "aliases": {
                "DarkSky": "Illuminated\\DarkSky\\DarkSky"
            }
        }
    }
}
